{
  "name": "formfiller-types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for FormFiller application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "pack-local": "npm pack",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "csfixer": "prettier --write src && eslint src --ext .ts --fix",
    "csfixer:check": "prettier --check src && eslint src --ext .ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "types",
    "typescript",
    "formfiller"
  ],
  "author": "Vivi and Sam <lowcoders@protonmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
